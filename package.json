{
  "name": "@rdietrick/sfdx-profile-decompose",
  "description": "Decomposes profile metadata into separate, more managable files.",
  "version": "0.0.1",
  "author": "Rob Dietrick @rdietrick",
  "bugs": "https://github.com/rdietrick/sfdx-profile-decompose/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/core": "^1.16.7",
    "@oclif/errors": "^1",
    "@salesforce/command": "^2",
    "@salesforce/core": "^3.31.9",
    "fast-xml-parser": "^3.16.0",
    "sinon": "^9.0.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/test": "^1",
    "@salesforce/cli-plugins-testkit": "^3.2.2",
    "@salesforce/dev-config": "1.4.1",
    "@salesforce/ts-sinon": "^1.4.0",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "chai": "^4",
    "eslint": "^8.25.0",
    "globby": "^11.0.4",
    "mocha": "^10.0.0",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.2",
    "ts-node": "^8",
    "tslint": "^5",
    "xml-query": "^1.5.0",
    "xml-reader": "^2.4.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/rdietrick/sfdx-profile-decompose",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "decompose": {
        "description": "Decompose metadata files into more modular files."
      },
      "aggregate": {
        "description": "Aggregate decomposed profile files into original XML format."
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "rdietrick/sfdx-profile-decompose",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
	"posttest": "eslint --ext .js,.ts .",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "mocha --timeout 10000 -r ts-node/register \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "index.d.ts"
}
